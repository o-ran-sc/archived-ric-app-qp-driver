{
    "xapp_name": "qp-driver",
    "version": "1.0.1",
    "containers": [
        {
            "name": "qp-driver",
            "image": {
                "registry": "nexus3.o-ran-sc.org:10002",
                "name": "o-ran-sc/ric-app-qp-driver",
                "tag": "1.0.1"
